                </telerik:RadRibbonGroup>
                <telerik:RadRibbonGroup>
                    <telerik:RadRibbonButton
                        x:Name="BtnManteniento"
                        Click="BtnManteniento_Click"
                        Content="Mantenimiento"
                        IsEnabled="True"
                        LargeImage="Resources/mantenimiento.png"
                        Size="Large" />
                    <telerik:RadRibbonButton
                        x:Name="BtnCreaPlantilla"
                        Click="MenuPrincipal"
                        IsEnabled="True"
                        LargeImage="Resources/duplicate.png"
                        Size="Large"
                        Tag="802">
                        <StackPanel Orientation="Vertical">
                            <TextBlock HorizontalAlignment="Center" Text="Generar" />
                            <TextBlock HorizontalAlignment="Center" Text="plantilla" />
                        </StackPanel>
                    </telerik:RadRibbonButton>


