                        LargeImage="Resources/ImaPadron.png"
                        Size="Large"
                        Tag="8">
                        <StackPanel Orientation="Vertical">
                            <TextBlock HorizontalAlignment="Center" Text="Ver" />
                            <TextBlock HorizontalAlignment="Center" Text="padrones" />
                        </StackPanel>
