                                    </telerik:RadMenuItem.Icon>
                                </telerik:RadMenuItem>
                                <telerik:RadMenuItem
                                    x:Name="BtnListaObraToExcel"
                                    Click="ObraTotExcel_Click"
                                    Header="Lista en Excel">
                                    <telerik:RadMenuItem.Icon>
                                        <Image Source="Resources/excel_32.png" Stretch="Fill" />
                                    </telerik:RadMenuItem.Icon>
                                </telerik:RadMenuItem>
                                <telerik:RadMenuItem
                                    x:Name="BtnListaObraToPdf"
                                    Click="ObraTotPdfClick"
