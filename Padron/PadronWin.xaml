        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="25" />
        </Grid.RowDefinitions>

        <telerik:RadRibbonView
            x:Name="BarraPrincipal"
            Margin="0,0,0,0"
            HorizontalAlignment="Stretch"
            VerticalAlignment="Stretch"
            ApplicationName="Padrón de Distribución del Semanario Judicial de la Federación"
            HelpButtonVisibility="Visible"
            IsMinimizable="False">
            <telerik:RadRibbonTab x:Name="Principal" Header="Principal">
                <telerik:RadRibbonGroup Header="Catálogos">
                    <telerik:RadRibbonButton
                        x:Name="BtnCatObras"
                        Click="MenuPrincipal"
                        Content="Obras"
                        LargeImage="Resources/catObras.png"
                        Size="Large"
                        Tag="1" />
                    <telerik:RadRibbonButton
                        x:Name="BtnCatOrganismos"
                        Click="MenuPrincipal"
                        Content="Organismos"
                        LargeImage="Resources/building.png"
                        Size="Large"
                        Tag="4" />
                    <telerik:RadRibbonButton
                        x:Name="BtnCatTitulares"
                        Click="MenuPrincipal"
                        Content="Titulares"
                        LargeImage="Resources/catTitulares.png"
                        Size="Large"
                        Tag="3" />
                    <telerik:RadRibbonButton
                        x:Name="BtnKiosko"
                        Click="MenuPrincipal"
                        Content="Kiosko"
                        LargeImage="Resources/ibooks-icon-200.png"
                        Size="Large"
                        Tag="2" />

